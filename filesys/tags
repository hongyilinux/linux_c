ftype	ftype.c	/^static int  ftype(const char *fname)  \/\/思考为什么不用char$/;"	f	file:
main	big.c	/^int main (int argc ,char **argv)$/;"	f
main	flen.c	/^int main (int argc ,char **argv)$/;"	f
main	ftype.c	/^int main (int argc,char **argv)$/;"	f
main	glob.c	/^int main (int argc,char **argv)$/;"	f
main	main.c	/^int main (int argc ,char *argv[])$/;"	f
